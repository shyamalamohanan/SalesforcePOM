		<classes>
			<class name="dashboardTestCases.CreateDashboard" />
			<class name="dashboardTestCases.EditDashboard" />
			<class name="dashboardTestCases.SubscribeDashboard" />
			<class name="dashboardTestCases.DeleteDashboard" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
